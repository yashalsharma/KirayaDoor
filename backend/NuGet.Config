<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!--
    Cursor sandbox/network restrictions may block nuget.org access.
    This file clears all package sources so the project can restore/build without external packages.

    When you're ready to add packages (EF Core, Swagger, etc.), either remove this file
    or add nuget.org back as a source.
  -->
  <packageSources>
    <clear />
    <add key="Package source 1" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
</configuration>

